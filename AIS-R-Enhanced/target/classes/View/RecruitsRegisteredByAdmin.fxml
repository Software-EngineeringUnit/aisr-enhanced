<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.Label?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="ais.RecruitsRegisteredByAdminController"
            prefHeight="400.0" prefWidth="400.0" maxWidth="400"  stylesheets="/styles/reviewallrecruits.css">
    <VBox spacing="10" layoutX="120.0"  alignment="CENTER">
        <Label text="Recruits Registered by Me" styleClass="registration-heading"/>
    </VBox>
    <TableView fx:id="recruitsTableView"  layoutX="50.0" layoutY="40.0" prefHeight="300.0" prefWidth="360.0" maxWidth="360.0">
        <columns>
            <TableColumn text="Full Name" fx:id="nameColumn" prefWidth="120"/>
            <TableColumn text="Qualification" fx:id="qualificationColumn" prefWidth="120"/>
            <TableColumn text="Date and Time" fx:id="dateColumn" prefWidth="120"/>
        </columns>     
    </TableView>
    <VBox alignment="CENTER" layoutX="200.0" layoutY="450.0"> 
        <Button text="I want to Go Back" onAction="#goBack" />
    </VBox>
</AnchorPane>

