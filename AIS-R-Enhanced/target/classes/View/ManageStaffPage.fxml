<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ais.ManageStaffPageController" stylesheets="styles/staffpage.css">
    <children>
        <VBox alignment="CENTER" layoutX="100.0" layoutY="50.0" spacing="20.0">
            <children>
                <Label text="Management Staff"  prefWidth="500"  styleClass="title-label" />
                <Button text="Register" onAction="#manageRegisterClicked" styleClass="main-button" />
                <Button text="Login" onAction="#manageLoginClicked" styleClass="main-button" />
                <Button text="I want to Go Back" onAction="#goBack" />
            </children>
        </VBox>
    </children>
</AnchorPane>
   
